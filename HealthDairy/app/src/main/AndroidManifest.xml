<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.blacklgames.healthdairy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".user_main_screen.UserMainActivity"
            android:label="@string/title_activity_user_main"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".add_receipt_screen.AddReceiptActivity"
            android:label="@string/title_activity_add_receipt"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".receipt_main_screen.ReceiptMainActivity"
            android:label="@string/title_activity_receipt_main"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".AddDrugActivity"
            android:label="@string/title_activity_add_drug"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>